<!-- ========= Canopy ============= -->
<canopy>
 <name>canopy</name>
 <mode>in-transit</mode>
 <path>Aircraft/Yak-55M/Sounds/Yak-55M_canopy.wav</path>
 <property>controls/canopy/canopy-pos-norm</property>
 <volume>
 <factor>0.12</factor>
 </volume>
   <position>
    <x>0.0</x>
    <y>0.0</y>
    <z>0.50</z>
   </position>
</canopy>

<click>
 <name>canopyCrash</name>
 <path>Aircraft/Yak-55M/Sounds/Yak-55M_canopy-crash.wav</path>
   <condition>
   <not>
   <property>control/canopy-not-crash</property>
   </not>
 </condition>   
 <volume>
 <factor>1.0</factor>
 </volume>
   <position>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.80</z>
   </position>
</click>



