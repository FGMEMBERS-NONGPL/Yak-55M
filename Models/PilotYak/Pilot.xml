<!-- Left arm animations -->


  <animation>
    <type>rotate</type>
    <object-name>Arm1L</object-name>
    <object-name>Arm3L</object-name>
    <object-name>Arm2L</object-name>	
    <property>controls/engines/engine/throttle</property>
    <factor>30.0</factor>
    <center>
      <x-m>0.372</x-m>
      <y-m>0.0</y-m>
      <z-m>0.409</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

 <animation>
    <type>rotate</type>
    <object-name>Arm2L</object-name>
    <object-name>Arm3L</object-name>
    <property>controls/engines/engine/throttle</property>
     <interpolation>
      <entry> <ind>0.0</ind> <dep>0</dep>  </entry>
      <entry> <ind>0.2</ind> <dep>-4</dep>  </entry>
      <entry> <ind>0.4</ind> <dep>-8</dep>  </entry>
      <entry> <ind>0.6</ind> <dep>-14</dep>  </entry>
      <entry> <ind>0.8</ind> <dep>-20</dep>  </entry>
      <entry> <ind>1.0</ind> <dep>-28</dep>  </entry>
     </interpolation>			
    <center>
      <x-m>0.491</x-m>
      <y-m>0.0</y-m>
      <z-m>0.171</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>


<!-- Right arm animations -->

  <animation>
