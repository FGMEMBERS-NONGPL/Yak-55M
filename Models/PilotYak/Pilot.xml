      <entry> <ind>-0.50</ind> <dep>  7</dep>  </entry>
      <entry> <ind>-0.25</ind> <dep>  3</dep>  </entry>
      <entry> <ind> 0.00</ind> <dep>  0</dep>  </entry>
      <entry> <ind> 0.25</ind> <dep> -6</dep>  </entry>
      <entry> <ind> 0.50</ind> <dep>-13</dep>  </entry>
      <entry> <ind> 0.75</ind> <dep>-19</dep>  </entry>
      <entry> <ind> 1.00</ind> <dep>-27</dep>  </entry>
     </interpolation>			
