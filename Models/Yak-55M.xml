	<nasal>
		<load>
		var livery_update = aircraft.livery_update.new("Aircraft/Yak-55M/Models/Liveries", 10);
		var self = cmdarg();
		var aero = self.getNode("sim/aero", 1);
		aero.alias(self.getNode("sim/multiplay/generic/string", 1));
