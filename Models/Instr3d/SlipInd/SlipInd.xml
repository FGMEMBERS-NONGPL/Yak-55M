<?xml version="1.0"?>

<PropertyList>

  <path>SlipInd.ac</path>

  <animation>
  <object-name>SlipBall1</object-name> 
  <object-name>SlipBall8</object-name>   
  <type>select</type> 
  <condition>
  <and>
  <greater-than>
  <property>orientation/roll-deg</property> 
  <value>-70</value> 
  </greater-than>
  <less-than>
  <property>orientation/roll-deg</property> 
  <value>70</value> 
  </less-than>
  </and>
  </condition>
  </animation>

  
  <animation>
  <object-name>SlipBall2</object-name> 
  <object-name>SlipBall7</object-name> 
  <type>select</type> 
  <condition>
  <or>
  <greater-than>
  <property>orientation/roll-deg</property> 
  <value>110</value> 
  </greater-than>
  <less-than>
  <property>orientation/roll-deg</property> 
  <value>-110</value>
  </less-than>  
  </or>
  </condition>
  </animation>
 

 <animation>
  <object-name>SlipBall3</object-name> 
  <object-name>SlipBall4</object-name>   
  <type>select</type> 
  <condition>
  <and>
  <greater-than>
  <property>orientation/roll-deg</property> 
  <value>-110</value> 
  </greater-than>
  <less-than>
  <property>orientation/roll-deg</property> 
  <value>-70</value> 
  </less-than>
  </and>
  </condition>
  </animation>

 <animation>
  <object-name>SlipBall5</object-name> 
  <object-name>SlipBall6</object-name>   
  <type>select</type> 
  <condition>
  <and>
  <greater-than>
  <property>orientation/roll-deg</property> 
  <value>70</value> 
  </greater-than>
  <less-than>
  <property>orientation/roll-deg</property> 
  <value>110</value> 
  </less-than>
  </and>
  </condition>
  </animation>


 
 
  
 <animation>
  <type>rotate</type>
  <object-name>SlipBall1</object-name>
  <property>fdm/jsbsim/accelerations/n-pilot-y-norm[0]</property>
      <interpolation>
        <entry><ind> -0.35</ind><dep> 17</dep></entry>
        <entry><ind> -0.3</ind><dep> 16.7</dep></entry>
        <entry><ind> -0.2</ind><dep> 11.31</dep></entry>
        <entry><ind> -0.1</ind><dep> 5.71</dep></entry>
        <entry><ind> 0</ind><dep> 0</dep></entry>
        <entry><ind> 0.1</ind><dep> -5.71</dep></entry>
        <entry><ind> 0.2</ind><dep> -11.31</dep></entry>
        <entry><ind> 0.3</ind><dep> -16.7</dep></entry>
        <entry><ind> 0.35</ind><dep> -17</dep></entry>
      </interpolation> 
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.05875</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>SlipBall2</object-name>
  <property>fdm/jsbsim/accelerations/n-pilot-y-norm[0]</property>
      <interpolation>
        <entry><ind> 0.35</ind><dep> 17</dep></entry>
        <entry><ind> 0.3</ind><dep> 16.7</dep></entry>
        <entry><ind> 0.2</ind><dep> 11.31</dep></entry>
        <entry><ind> 0.1</ind><dep> 5.71</dep></entry>
        <entry><ind> 0</ind><dep> 0</dep></entry>
        <entry><ind> -0.1</ind><dep> -5.71</dep></entry>
        <entry><ind> -0.2</ind><dep> -11.31</dep></entry>
        <entry><ind> -0.3</ind><dep> -16.7</dep></entry>
        <entry><ind> -0.35</ind><dep> -17</dep></entry>
      </interpolation> 
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.042</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 
 
 

</PropertyList> 
